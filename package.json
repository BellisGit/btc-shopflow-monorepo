    "deploy:quality": "bash scripts/deploy-app-local.sh quality-app",
    "deploy:production": "bash scripts/deploy-app-local.sh production-app",
    "deploy:engineering": "bash scripts/deploy-app-local.sh engineering-app",
    "deploy:finance": "bash scripts/deploy-app-local.sh finance-app",
    "deploy:mobile": "bash scripts/deploy-app-local.sh mobile-app",
