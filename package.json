    "build-deploy:finance:full": "pnpm build:finance && pnpm deploy:finance && pnpm deploy:static:finance",
    "build-deploy:mobile:full": "pnpm build:mobile && pnpm deploy:mobile && pnpm deploy:static:mobile",
    "build-deploy:k8s": "bash scripts/build-deploy-incremental-k8s.sh",
    "build-deploy:k8s:all": "bash scripts/build-deploy-incremental-k8s.sh --all",
    "build-deploy:k8s:system": "bash scripts/build-deploy-incremental-k8s.sh --apps system-app",
    "build-deploy:k8s:admin": "bash scripts/build-deploy-incremental-k8s.sh --apps admin-app",
    "build-deploy:k8s:logistics": "bash scripts/build-deploy-incremental-k8s.sh --apps logistics-app",
    "build-deploy:k8s:quality": "bash scripts/build-deploy-incremental-k8s.sh --apps quality-app",
