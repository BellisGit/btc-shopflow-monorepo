  "scripts": {
    "prepare": "echo 'Husky disabled - manual checks only'",
    "preinstall": "echo 'Skipping only-allow check'",
    "check:circular": "node scripts/check-circular-deps.mjs",
    "dev": "chcp 65001 >nul && concurrently -n \"admin,logistics,system,finance,engineering,quality,production,docs,monitor\" -c \"bgBlue.bold,bgGreen.bold,bgCyan.bold,bgMagenta.bold,bgYellow.bold,bgRed.bold,bgMagenta.bold,bgCyan.bold,bgYellow.bold\" \"pnpm run dev:admin\" \"pnpm run dev:logistics\" \"pnpm run dev:system\" \"pnpm run dev:finance\" \"pnpm run dev:engineering\" \"pnpm run dev:quality\" \"pnpm run dev:production\" \"pnpm run dev:docs\" \"pnpm run dev:monitor\"",
    "predev:all": "node scripts/turbo.js run build --filter=@btc/vite-plugin --filter=@btc/shared-utils --filter=@btc/shared-core --filter=@btc/shared-components --filter=@btc/subapp-manifests --force",
    "dev:all": "node scripts/turbo.js run dev --concurrency=20",
    "dev:admin": "pnpm --filter admin-app dev",
    "dev:logistics": "pnpm --filter logistics-app dev",
    "dev:system": "pnpm --filter system-app dev",
    "dev:finance": "pnpm --filter finance-app dev",
    "dev:engineering": "pnpm --filter engineering-app dev",
    "dev:quality": "pnpm --filter quality-app dev",
    "dev:production": "pnpm --filter production-app dev",
    "dev:docs": "pnpm --filter docs-site-app dev",
    "dev:monitor": "pnpm --filter monitor-app dev",
    "dev:mobile": "pnpm --filter mobile-app dev",
    "prebuild:all": "node scripts/clean-cache.mjs",
    "prebuild": "pnpm check:circular",
    "build:all": "node scripts/turbo.js run build",
    "build-dist:all": "node scripts/build-to-dist.mjs",
    "copy:eps": "node scripts/copy-eps-from-system.mjs",
    "build:system": "cd apps/system-app && node scripts/build.js build",
    "preview:system": "cd apps/system-app && node scripts/build.js preview",
    "build-preview:system": "cd apps/system-app && node scripts/build.js build && node scripts/build.js preview",
    "build:admin": "pnpm --filter admin-app build",
    "preview:admin": "pnpm --filter admin-app preview",
    "build-preview:admin": "pnpm build:admin && pnpm preview:admin",
    "build:logistics": "pnpm --filter logistics-app build && node scripts/verify-build-assets.mjs logistics-app",
    "preview:logistics": "pnpm --filter logistics-app preview",
    "build-preview:logistics": "pnpm build:logistics && pnpm preview:logistics",
