{
  "name": "btc-shopflow-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "BTC ShopFlow 微前端项目",
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=8.0.0"
