    "build-deploy:k8s:engineering": "bash scripts/build-deploy-incremental-k8s.sh --apps engineering-app",
    "build-deploy:k8s:finance": "bash scripts/build-deploy-incremental-k8s.sh --apps finance-app",
    "build-deploy:k8s:mobile": "bash scripts/build-deploy-incremental-k8s.sh --apps mobile-app",
    "build:k8s": "bash scripts/build-incremental-k8s.sh",
    "build:k8s:all": "bash scripts/build-incremental-k8s.sh --all",
    "build:k8s:system": "bash scripts/build-incremental-k8s.sh --apps system-app",
    "build:k8s:admin": "bash scripts/build-incremental-k8s.sh --apps admin-app",
    "build:k8s:logistics": "bash scripts/build-incremental-k8s.sh --apps logistics-app",
    "build:k8s:quality": "bash scripts/build-incremental-k8s.sh --apps quality-app",
    "build:k8s:production": "bash scripts/build-incremental-k8s.sh --apps production-app",
    "build:k8s:engineering": "bash scripts/build-incremental-k8s.sh --apps engineering-app",
    "build:k8s:finance": "bash scripts/build-incremental-k8s.sh --apps finance-app",
    "build:k8s:mobile": "bash scripts/build-incremental-k8s.sh --apps mobile-app",
