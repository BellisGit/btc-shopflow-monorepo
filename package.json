    "build-deploy:production": "bash scripts/build-and-push-local.sh production-app --auto-deploy",
    "build-deploy:engineering": "bash scripts/build-and-push-local.sh engineering-app --auto-deploy",
    "build-deploy:finance": "bash scripts/build-and-push-local.sh finance-app --auto-deploy",
    "build-deploy:mobile": "bash scripts/build-and-push-local.sh mobile-app --auto-deploy",
