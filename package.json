    "test:integration": "pnpm --filter admin-app test:integration",
    "test:e2e": "pnpm exec playwright test",
    "test:ci": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:deployment": "node scripts/test-deployment.mjs",
    "test:deployment:all": "node scripts/test-deployment.mjs --all",
    "test:deployment:app": "node scripts/test-deployment.mjs --app",
    "format": "prettier --write \"packages/**/*.{ts,tsx,vue,json,md}\"",
    "type-check": "node scripts/turbo.js run type-check",
    "tsc:all": "node scripts/turbo.js run type-check",
    "tsc:admin": "pnpm --filter admin-app type-check",
    "tsc:logistics": "pnpm --filter logistics-app type-check",
    "tsc:system": "pnpm --filter system-app type-check",
    "tsc:finance": "pnpm --filter finance-app type-check",
    "tsc:engineering": "pnpm --filter engineering-app type-check",
    "tsc:quality": "pnpm --filter quality-app type-check",
    "tsc:production": "pnpm --filter production-app type-check",
    "tsc:mobile": "pnpm --filter mobile-app type-check",
