    "deploy:k8s:system": "bash scripts/deploy-incremental-k8s.sh --apps system-app",
    "deploy:k8s:admin": "bash scripts/deploy-incremental-k8s.sh --apps admin-app",
    "deploy:k8s:logistics": "bash scripts/deploy-incremental-k8s.sh --apps logistics-app",
    "deploy:k8s:quality": "bash scripts/deploy-incremental-k8s.sh --apps quality-app",
    "deploy:k8s:production": "bash scripts/deploy-incremental-k8s.sh --apps production-app",
    "deploy:k8s:engineering": "bash scripts/deploy-incremental-k8s.sh --apps engineering-app",
