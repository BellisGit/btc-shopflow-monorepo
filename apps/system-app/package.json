{
  "name": "system-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "src/services/eps.ts",
    "src/App.vue",
    "**/*.vue",
    "virtual:eps"
  ],
  "scripts": {
    "predev": "pnpm --filter @btc/vite-plugin build && pnpm check:circular",
    "dev": "vite --port 8080",
    "build": "node scripts/build.js build",
    "preview": "node scripts/build.js preview",
    "pretype-check": "pnpm --filter @btc/shared-utils run build && pnpm --filter @btc/shared-core run build && pnpm --filter @btc/shared-components run build",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@btc/shared-components": "^1.0.0",
    "@btc/shared-core": "^1.0.0",
    "@btc/shared-utils": "^1.0.0",
    "@btc/subapp-manifests": "^0.0.1",
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "chardet": "^2.1.0",
    "echarts": "^6.0.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "qiankun": "^2.10.16",
    "vue": "^3.4.0",
    "vue-echarts": "^8.0.1",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@btc/vite-plugin": "^1.0.0",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "element-plus": "^2.13.0",
    "jquery": "3.7.1",
    "sass": "^1.93.2",
    "typescript": "^5.3.3",
    "unocss": "^66.5.2",
    "vite": "^5.0.0",
    "vite-plugin-qiankun": "^1.0.15",
    "vue-tsc": "^1.8.27"
  }
}
