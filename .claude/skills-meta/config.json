{
  "optimization_thresholds": {
    "min_success_rate": 0.8,
    "min_avg_rating": 3.5,
    "min_multi_dimension_scores": {
      "accuracy": 3.5,
      "completeness": 3.5,
      "efficiency": 3.5,
      "usability": 3.5,
      "practicality": 3.5
    },
    "max_avg_iterations": 3
  },
  "feedback_analysis": {
    "enable_sentiment_analysis": true,
    "enable_auto_inference": true,
    "min_feedback_confidence": 0.6,
    "ask_question_enabled": true
  },
  "hint_system": {
    "enable_hints": true,
    "first_execution_mode": true,
    "detailed_questioning_enabled": true,
    "hint_accuracy_threshold": 0.5,
    "hint_success_threshold": 0.8,
    "min_hint_confidence": 0.6,
    "hint_weight_increment": 0.1,
    "hint_weight_decrement": 0.1
  },
  "skill_splitting": {
    "enable_splitting": true,
    "max_steps_before_split": 10,
    "max_duration_before_split": 1800,
    "min_step_reuse_count": 3,
    "split_effectiveness_threshold": 0.2
  },
  "step_scoring": {
    "enable_step_tracking": true,
    "critical_step_weight": 2.0,
    "normal_step_weight": 1.0,
    "min_step_score_threshold": 0.7
  },
  "analysis_window": 20,
  "auto_optimize_enabled": true,
  "backup_before_optimize": true,
  "command_interception": {
    "enabled": true,
    "auto_track": true,
    "track_on_start": true,
    "track_on_complete": true
  },
  "parallel_execution": {
    "enabled": true,
    "auto_open_chat": true,
    "shortcut": "Ctrl+L",
    "step_threshold": 5,
    "delay_between_chats": 2000
  },
  "data_collection": {
    "capture_project_state": true,
    "log_namespace_prefix": "agent-skill",
    "implicit_feedback_enabled": true
  },
  "root_cause_analysis": {
    "enable_auto_attribution": true,
    "min_confidence_threshold": 0.7,
    "max_analysis_depth": 3
  },
  "skill_versioning": {
    "enable_versioning": true,
    "auto_backup": true,
    "max_versions": 10
  },
  "validation": {
    "enable_pre_update_validation": true,
    "test_data_sources": ["historical_executions", "conversation_scenarios"],
    "min_improvement_threshold": 0.05
  },
  "iteration_scheduling": {
    "enable_auto_iteration": true,
    "scheduled_optimization": "weekly",
    "threshold_triggers": {
      "success_rate": 0.8,
      "avg_rating": 3.5
    },
    "scenario_triggers": ["new_microfrontend", "package_structure_change"]
  },
  "gradual_rollout": {
    "enable_gradual_rollout": true,
    "initial_rollout_percentage": 10,
    "rollout_increment": 10,
    "min_validation_period_hours": 24
  }
}
