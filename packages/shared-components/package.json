  "devDependencies": {
    "@btc/shared-core": "workspace:*",
    "@btc/shared-utils": "workspace:*",
    "@element-plus/icons-vue": "^2.3.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "element-plus": "^2.11.5",
    "sass": "^1.93.2",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
