  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "prebuild": "node ../../scripts/check-src-artifacts.mjs packages/shared-core && node scripts/generate-overview-i18n.mjs",
    "build": "vite build && node scripts/create-dist-src-index.mjs",
    "postbuild": "node scripts/create-dist-src-index.mjs",
    "type-check": "tsc --noEmit --project tsconfig.type-check.json"
  },
  "peerDependencies": {
    "@vueuse/core": "^12.0.0 || ^13.0.0",
    "pinia": "^2.0.0 || ^3.0.0",
    "qiankun": "^2.10.16",
    "vue": "^3.3.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "dexie": "^4.2.1",
    "file-type": "^21.2.0",
    "pinia-plugin-persistedstate": "^3.2.3",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@build-utils/logger": "workspace:*",
    "@types/pino": "^7.0.5",
    "@vueuse/core": "^13.9.0",
    "element-plus": "2.13.0",
    "pinia": "^3.0.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.20",
    "vite-plugin-dts": "^4.5.4",
    "vue-router": "^4.0.0"
