      "types": "./src/utils/form/zod-validator.d.ts",
      "development": "./src/utils/form/zod-validator.ts",
      "import": "./dist/utils/form/zod-validator.mjs",
      "require": "./dist/utils/form/zod-validator.js"
    },
    "./utils/format": {
      "types": "./src/utils/format/index.d.ts",
      "development": "./src/utils/format/index.ts",
      "import": "./dist/utils/format/index.mjs",
      "require": "./dist/utils/format/index.js"
    },
    "./utils/qiankun/load-layout-app": {
      "types": "./src/utils/qiankun/load-layout-app.d.ts",
