{
  "name": "@btc/subapp-manifests",
  "version": "0.0.1",
  "type": "module",
  "description": "BTC ShopFlow 子应用清单配置",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "node ../../scripts/run-without-env.js tsup src/index.ts --format esm --dts --clean",
    "prepare": "node ../../scripts/run-without-env.js tsup src/index.ts --format esm --dts --clean"
  },
  "devDependencies": {
    "element-plus": "2.13.0",
    "tsup": "^7.3.0",
    "typescript": "^5.3.3"
  }
}
